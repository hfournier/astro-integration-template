---
import type { z } from "astro:content";
import type { ogAudioSchema } from "../../src/seo.js";

type Props = {
	audio: z.infer<typeof ogAudioSchema>;
};
const { audio } = Astro.props;
---

{audio.url && <meta property="og:audio" content={audio.url} />}
{audio.mimeType && <meta property="og:audio:type" content={audio.mimeType} />}
